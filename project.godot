; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="SCREAM Secret Game Jam"
run/main_scene="res://Scenes/World/main_menu.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

GlobalManager="*res://Scripts/Managers/global_manager.gd"
InventoryUi="*res://Scenes/World/inventory_ui.tscn"
DialogueUi="*res://Scenes/World/dialogue_ui.tscn"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="canvas_items"

[file_customization]

folder_colors={
"res://Audio/": "yellow",
"res://Materials/": "green",
"res://Scenes/": "red",
"res://Scripts/": "blue",
"res://Sprites/": "purple"
}
